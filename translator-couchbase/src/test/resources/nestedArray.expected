SET NAMESPACE 'http://www.teiid.org/translator/couchbase/2017' AS teiid_couchbase;

CREATE FOREIGN TABLE nestedArray (
	"_documentId" string OPTIONS (UPDATABLE FALSE),
	nestedArray_idx integer OPTIONS (UPDATABLE FALSE),
	nestedArray string OPTIONS (NAMEINSOURCE '`nestedArray`[]'),
	FOREIGN KEY("_documentId") REFERENCES test 
) OPTIONS (NAMEINSOURCE '`nestedArray`[]', UPDATABLE TRUE, "teiid_couchbase:ISARRAYTABLE" 'true');

CREATE FOREIGN TABLE nestedArray_dim1 (
	"_documentId" string OPTIONS (UPDATABLE FALSE),
	nestedArray_dim1_idx integer OPTIONS (UPDATABLE FALSE),
	nestedArray_dim1 string OPTIONS (NAMEINSOURCE '`nestedArray`[][]'),
	FOREIGN KEY("_documentId") REFERENCES test 
) OPTIONS (NAMEINSOURCE '`nestedArray`[][]', UPDATABLE TRUE, "teiid_couchbase:ISARRAYTABLE" 'true');

CREATE FOREIGN TABLE nestedArray_dim1_dim2 (
	"_documentId" string OPTIONS (UPDATABLE FALSE),
	nestedArray_dim1_dim2_idx integer OPTIONS (UPDATABLE FALSE),
	nestedArray_dim1_dim2 string OPTIONS (NAMEINSOURCE '`nestedArray`[][][]'),
	FOREIGN KEY("_documentId") REFERENCES test 
) OPTIONS (NAMEINSOURCE '`nestedArray`[][][]', UPDATABLE TRUE, "teiid_couchbase:ISARRAYTABLE" 'true');

CREATE FOREIGN TABLE nestedArray_dim1_dim2_dim3 (
	"_documentId" string OPTIONS (UPDATABLE FALSE),
	nestedArray_dim1_dim2_dim3_idx integer OPTIONS (UPDATABLE FALSE),
	nestedArray_dim1_dim2_dim3 string OPTIONS (NAMEINSOURCE '`nestedArray`[][][][]'),
	FOREIGN KEY("_documentId") REFERENCES test 
) OPTIONS (NAMEINSOURCE '`nestedArray`[][][][]', UPDATABLE TRUE, "teiid_couchbase:ISARRAYTABLE" 'true');

CREATE FOREIGN TABLE test (
	"_documentId" string OPTIONS (UPDATABLE FALSE),
	Name string OPTIONS (NAMEINSOURCE '`Name`'),
	CONSTRAINT PK0 PRIMARY KEY("_documentId")
) OPTIONS (NAMEINSOURCE '`test`', UPDATABLE TRUE, "teiid_couchbase:ISARRAYTABLE" 'false');